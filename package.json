{
    "name": "schedulewizard2",
    "version": "1.0.0",
    "description": "",
    "main": "server/server.js",
    "scripts": {
        "start": "node server/server.js",
        "develop": "concurrently \"npm run server-watch\" \"npm run client-dev\"",
        "postinstall": "npm run server-install && npm run client-install",
        "server-install": "cd server && npm install",
        "client-install": "cd client && npm install",
        "server-watch": "cd server && nodemon",
        "client-dev": "cd client && vite",
        "seed": "cd server && npm run seed",
        "build": "npm run client-build",
        "client-build": "cd client && vite build",
        "render-build": "npm install && npm run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "stripe": "^16.2.0",
        "concurrently": "^8.2.2",
        "@vitejs/plugin-react": "^4.3.1",
        "vite": "^5.3.5"
    },
    "devDependencies": {
        "nodemon": "^3.1.1"
    }
}
